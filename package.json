  "jest": {
    "projects": [
      "app/jest.client.config.js",
      "app/jest.server.config.js"
    ]
  },
  "scripts": {
    "production-build": "./node_modules/.bin/cross-env NODE_ENV=production webpack --config ./webpack.production.config.js --progress --profile --colors",
    "run-production": "node ./node_modules/cross-env/dist/bin/cross-env.js NODE_ENV=production && node server.js",
    "dev-server": "./node_modules/.bin/nodemon --ignore 'app/dist/*' --watch 'app/api' --watch 'app/shared' --watch 'app/react/ServerRouter.js'",
    "webpack-server": "node ./webpack/webpack.server.js",
    "hot": "./node_modules/.bin/cross-env  HOT=true  && npm run dev-server && npm run webpack-server",
    "analyze": "webpack --profile --json > stats.json;./node_modules/.bin/webpack-bundle-analyzer stats.json",
    "download-mongo-prebuilt": "node ./database/download_mongo_prebuilt.js",
    "postinstall": "node -e \"process.env.NODE_ENV != 'production' && process.exit(1)\" || npm run download-mongo-prebuilt",
    "reindex": "node run.js ./database/reindex_elastic.js",
    "health-check": "node run.js ./database/health_check.js",
    "blank-state": "./database/blank_state.sh; yarn reindex",
    "fixtures": "cd uwazi-fixtures; ./restore.sh; yarn reindex",
    "add-migration": "./node_modules/.bin/plop --plopfile ./app/api/migrations/plopfile.js migration",
    "migrate": "node run.js ./app/api/migrations/migrate.js",
    "test-debug": "node --inspect ./node_modules/.bin/jest --watch -i",
    "e2e": "node nightmare_tests.js",
    "eslint": "./node_modules/.bin/eslint app --quiet"
  },

{
  "name": "Uwazi",
  "version": "1.1.1",
  "description": "Uwazi is a free, open-source solution for organising, analysing and publishing your documents.",
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:huridocs/uwazidocs.git"
  },
  "keywords": [
    "react"
